//Амулет: Кольцо люцифера
var Lucifer_Ring = VanillaFactory.createBaubleItem("Lucifer_Ring");
Lucifer_Ring.rarity = "EPIC";
Lucifer_Ring.onWornTick = function(bauble, wearer) {
    if(wearer instanceof IPlayer) {
        var player as IPlayer = wearer;
             player.addPotionEffect(<potion:minecraft:water_breathing>.makePotionEffect(5, 0));
             player.addPotionEffect(<potion:minecraft:fire_resistance>.makePotionEffect(5, 0));
             player.addPotionEffect(<potion:minecraft:haste>.makePotionEffect(5, 5));
             player.addPotionEffect(<potion:minecraft:absorption>.makePotionEffect(50, 5));
             player.addPotionEffect(<potion:minecraft:regeneration>.makePotionEffect(50, 5));
             player.addPotionEffect(<potion:minecraft:strength>.makePotionEffect(25, 100));
             player.addPotionEffect(<potion:minecraft:health_boost>.makePotionEffect(45, 5));
             player.addPotionEffect(<potion:minecraft:resistance>.makePotionEffect(5, 20));
                
    }
};
Lucifer_Ring.baubleType = "RING";
Lucifer_Ring.register();







