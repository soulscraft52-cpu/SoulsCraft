//Амулет: Сердце зверя
var The_Heart_of_the_Beast = VanillaFactory.createBaubleItem("The_Heart_of_the_Beast");
The_Heart_of_the_Beast.rarity = "EPIC";
The_Heart_of_the_Beast.onWornTick = function(bauble, wearer) {
    if(wearer instanceof IPlayer) {
        var player as IPlayer = wearer;
          player.addPotionEffect(<potion:minecraft:regeneration>.makePotionEffect(50, 5));
    }
};
The_Heart_of_the_Beast.baubleType = "TRINKET";
The_Heart_of_the_Beast.register();

